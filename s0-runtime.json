{
  "target": "bmv2",
  "p4info": "build/frontend.p4.p4info.txt",
  "bmv2_json": "build/frontend.json",
  "clone_session_entries": [
    {
      "clone_session_id": 5,
      "replicas": [
          {"egress_port": 4,
          "instance": 1
        }
      ]
    }
  ],
  "table_entries": [
    {
      "table": "MyIngress.clone_table",
      "default_action": true,
      "action_name": "MyIngress.noAction",
      "action_params": { }
    },
    {
      "table": "MyIngress.clone_table",
      "match": {
        "hdr.request.reqType": 1
      },
      "action_name": "MyIngress.clone_pkt",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.request.small_key": 1
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:02:00",
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.request.small_key": 0
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:00",
        "port": 3
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.request.small_key": 2
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:00",
        "port": 1
      }
    }

  ]
}
